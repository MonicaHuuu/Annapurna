j = 1

for i in range(0,10):
    if i < j:
        i += 1
        continue
    else:
        for j in range(1,i+1):
            if i > j:
                print("%d*%d="%(i,j),end="")
                print(i*j,end="\t")
            elif i == j:
                print("%d*%d="%(i,j),end="")
                print(i*j,end="\n")
